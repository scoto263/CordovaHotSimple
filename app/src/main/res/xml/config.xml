<?xml version='1.0' encoding='utf-8'?>
<widget  xmlns="http://www.w3.org/ns/widgets" xmlns:cdv="http://cordova.apache.org/ns/1.0">
    <preference name="loglevel" value="DEBUG" />
    <allow-intent href="market:*" />
    <preference name="loadUrlTimeoutValue" value="10000" />
    <feature name="Whitelist">
        <param name="android-package" value="com.nordnetab.chcp.main.WhitelistPlugin" />
        <param name="onload" value="true" />
    </feature>
    <feature name="HotCodePush">
        <param name="android-package" value="com.nordnetab.chcp.main.HotCodePushPlugin" />
        <param name="onload" value="true" />
    </feature>
    <chcp>
        <auto-download enabled="false" />
        <auto-install enabled="true" />
        <config-file url="http://192.168.1.118:6008/Appversion/findNewRecord" />
    </chcp>
    <content src="index.html" />
    <access origin="*" />
    <allow-intent href="http://*/*" />
    <allow-intent href="https://*/*" />
    <allow-intent href="tel:*" />
    <allow-intent href="sms:*" />
    <allow-intent href="mailto:*" />
    <allow-intent href="geo:*" />
</widget>
